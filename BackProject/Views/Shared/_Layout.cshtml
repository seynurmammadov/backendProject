                                @if (User.Identity.IsAuthenticated)
                                {
                                    <li>
                                        <a role="button">
                                            @User.Identity.Name
                                        </a>
                                    </li>
                                    <li>
                                        <a asp-controller="register" asp-action="logout">
                                            LogOut
                                        </a>
                                    </li>

                                }
                                else
                                {
                                    <li><a asp-controller="login" asp-action="index">login</a></li>
                                    <li><a asp-controller="register" asp-action="index">register</a></li>
                                }

                            </ul>
